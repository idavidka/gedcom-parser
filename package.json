{
	"name": "@treeviz/gedcom-parser",
	"version": "1.0.0",
	"description": "GEDCOM parser library for TreeViz - Parse and manipulate GEDCOM genealogy files",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"sideEffects": false,
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint --fix src/**/*.ts",
		"test": "echo \"Tests will be migrated from main repo\" && exit 0",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"gedcom",
		"parser",
		"genealogy",
		"family-tree",
		"treeviz"
	],
	"author": "idavidka",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/idavidka/gedcom-parser.git"
	},
	"dependencies": {
		"date-fns": "^2.30.0",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@types/lodash": "^4.17.21",
		"@types/node": "^20.19.27",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
