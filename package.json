{
  "name": "@treeviz/gedcom-parser",
  "version": "1.0.0",
  "description": "GEDCOM parser library for TreeViz - Parse and handle GEDCOM files",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch"
