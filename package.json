  "repository": {
    "type": "git",
    "url": "git@github.com:idavidka/gedcom-parser.git"
  },
  "keywords": [
    "gedcom",
    "genealogy",
    "parser",
    "family-tree",
    "typescript"
  ],
  "author": "idavidka",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.17.23",
    "typescript": "^5.6.3"
  }
